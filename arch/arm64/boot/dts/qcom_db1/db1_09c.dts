
#include "apq8016-sbc.dts"

/*
 DB1 with 9.7" capacitive touch screen (ilitek_touch)
*/

/* Working frequency: 100MHz */
&lvds {
	clock-frequency = <100000000>;
	hactive = <1024>;
	vactive = <768>;
	hback-porch = <250>;
	hsync-len = <500>;
	hfront-porch = <250>;
	vback-porch = <6>;
	vfront-porch = <16>;
	vsync-len = <10>;
	hsync-active = <0>;
	vsync-active = <0>;
	de-active = <1>;
	pixelclk-active = <0>;
};


